    <!-- Top-->
    <h1 class="site-heading text-center text-black d-none d-lg-block">
      <span class="site-heading-upper text-primary mb-3">Y O R O</span>
      <span class="site-heading-lower">
        <span style="color:#e6a756">Y</span>ou <span style="color:#e6a756">O</span>nly <span style="color:#e6a756">R</span>ank <span style="color:#e6a756">O</span>nce!
      </span>
    </h1>

    <!-- Banner -->
    <nav class="navbar navbar-expand-lg navbar-dark py-lg-4" id="mainNav">
      <div class="container">
        <a class="navbar-brand text-uppercase text-expanded font-weight-bold d-lg-none" href="#">YORO</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
          <ul class="navbar-nav mx-auto">
            <li class="nav-item px-lg-4">
              <a class="nav-link text-uppercase text-expanded" href="#yoro_play">학생 점수 입력하기</a>
            </li>
      
          </ul>
        </div>
      </div>
    </nav>
    
    <!--교수 입력 플랫폼-->
    <section class="page-section cta" id="page-section cta">
      <div class="container" id="yoro_play">
        <div class="row">
          <div class="col-xl-9 mx-auto">
            <div class="cta-inner text-center rounded">
              <h2 class="section-heading mb-4">
                <span class="section-heading-upper">학점을 확인해보세요!</span>
                <span class="section-heading-lower">To You</span>
              </h2>
              <!--form 태그-->
              <form action="/home/result" method="post">
                <%= hidden_field_tag :authenticity_token, form_authenticity_token, id: "token" %>
                 <div class="input-group mb-3">
                  <div class="input-group-prepend">
                   <span class="input-group-text" id="basic-addon1"><i class="fa fa-user-o" aria-hidden="true"></i></span>
                </div>
                
                  <input type="integer" id="st_number" class="form-control" placeholder="학번" aria-label="학번" aria-describedby="basic-addon1"><br>
                  <input type="integer" id="st_birthday" class="form-control" placeholder="생년월일 6자리" aria-label="생년월일 6자리" aria-describedby="basic-addon1"><br>
                  <input type="integer" id="sub_number" class="form-control" placeholder="과목코드" aria-label="과목코드" aria-describedby="basic-addon1"><br>
                  &nbsp<button type="submit" class="btn btn-warning" onclick="javascript:btn()">확인하기</button>
                </div>
                </form>
                 
           </div>
          </div>
        </div>
      </div>
    </section>
    
    <footer class="footer text-faded text-center py-5">
      <div class="container">
        <p class="m-0 small">Copyright &copy; YORO 2018년 멋쟁이사자처럼 뉴비톤<br>
        대학교육프로세스 3팀<br>
        프론트엔드 : 김보배(동국대) , 이진규(성결대)<br>
        백엔드 : 김명석(성공회대) , 이지민(동국대)</p>
      </div>
    </footer>
    
    
