    <!--경고창-->
    <script>
      function btn(){
    
    //name으로 접근하여 가져오기

    var input1=document.getElementById('st_number').value;
    var input2=document.getElementById('sub_number').value;
    var input3=document.getElementById('st_birthday').value;
    
    var db1=$('.temp_st').data('temp');
    var db2=$('.temp_sb').data('temp');
    var db3=$('.temp_birth').data('temp');
    
    var result1 = false;
    var result2 = false;
    var result3 = false;
    
    
    for (var i = 0; i < db1.length; i++) { 
      if (db1[i]==input1){
        result1 = true;
        break;
      }
    
    }
    
    
    for (var j = 0; j < db2.length; j++) { 
      if (db2[j]==input2){
        result2 = true;
        break;
      }
    
    }
    
    for (var k = 0; k < db3.length; k++) { 
      if (db3[k]==input3){
        result3 = true;
        break;
      }
    
    }
    
    var token=document.getElementById('token').value;
    
    
    if(result1&&result2&&result3){
      // location.href="/home/result";
      // method = method || "post"; // post (set to default) or get

        	// Create the form object
        	var form = document.createElement("form");
        	form.setAttribute("method", "post");
        	form.setAttribute("action", "/home/result");
        	
        	
        	  var hiddenField = document.createElement("input");	
        		hiddenField.setAttribute("type", "hidden"); 
        		hiddenField.setAttribute("name", "authenticity_token");
        		hiddenField.setAttribute("value", token);
        		// append the newly created control to the form
        		form.appendChild(hiddenField); 
        	
        		var hiddenField = document.createElement("input");	
        		hiddenField.setAttribute("type", "hidden"); 
        		hiddenField.setAttribute("name", "st_number");
        		hiddenField.setAttribute("value", input1);
        		// append the newly created control to the form
        		form.appendChild(hiddenField); 
        	
        	
        	  
        		var hiddenField = document.createElement("input");	
        		hiddenField.setAttribute("type", "hidden"); 
        		hiddenField.setAttribute("name", "st_birthday");
        		hiddenField.setAttribute("value", input3);
        		// append the newly created control to the form
        		form.appendChild(hiddenField);
        		
        		
        		var hiddenField = document.createElement("input");	
        		hiddenField.setAttribute("type", "hidden"); 
        		hiddenField.setAttribute("name", "sub_number");
        		hiddenField.setAttribute("value", input2);
        		// append the newly created control to the form
        		form.appendChild(hiddenField);
        
        	document.body.appendChild(form); // inject the form object into the body section
        	form.submit();
        	
    }else{
      
      alert(' 입력한 정보가 틀렸습니다. 정보를 다시 입력해주세요.');
      
    }
}

    </script>
